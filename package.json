{
  "name": "minibase-better-define",
  "version": "0.2.0",
  "description": "Plugin for [base][] and [minibase][] that overrides the core `.define` method to be more better.",
  "repository": "node-minibase/minibase-better-define",
  "author": "Charlike Mike Reagent <@tunnckoCore> (http://www.tunnckocore.tk)",
  "precommit.silent": true,
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "standard --verbose",
    "pretest": "npm run lint",
    "test": "npm run coverage",
    "posttest": "npm run lint:coverage",
    "coverage": "nyc node test.js",
    "lint:coverage": "nyc check-coverage --lines 100 --branches 100 --statements 100 --functions 100",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "prerelease": "npm test",
    "release": "standard-version --sign --no-verify",
    "precommit": "git add --all",
    "commit": "git-cz"
  },
  "dependencies": {
    "define-property": "^0.2.5",
    "isobject": "^2.1.0",
    "lazy-cache": "^2.0.1",
    "minibase-is-registered": "^1.1.0"
  },
  "devDependencies": {
    "commitizen": "^2.8.6",
    "coveralls": "^2.11.14",
    "cz-conventional-changelog": "^1.2.0",
    "minibase": "^0.4.1",
    "mukla": "^0.4.5",
    "nyc": "^8.3.2",
    "pre-commit": "^1.1.3",
    "standard": "^8.5.0",
    "standard-version": "^3.0.0"
  },
  "files": [
    "index.js",
    "utils.js"
  ],
  "keywords": [],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "verb": {
    "run": true,
    "toc": true,
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "minibase",
        "minibase-is-registered",
        "minibase-assert",
        "minibase-visit",
        "try-catch-core",
        "always-done",
        "mukla"
      ]
    },
    "reflinks": [
      "base",
      "define-property",
      "minibase",
      "always-done",
      "assert-kindof",
      "async-done",
      "collection-visit",
      "dezalgo",
      "once"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
